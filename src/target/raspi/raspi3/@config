/* plateform config */
#define ARCH_PERIPHERAL_BASE 0x3F000000
#define ARCH_CONTROL_BASE    0x40000000
#define ARCH_ENTRY_POINT     0x80000
#define ARCH_RELEASE         0x3
#define ARCH_NAME            "raspberry3"

/* Arch config */
#define ARCH_DIMENSION 0x8

/* kconfig */
#define KCONFIG_MAXCPUS     0x4
#define KCONFIG_MAXTHREADS  0x20

#define KCONFIG_MMU_PAGESIZE 0x1000

#define KCONFIG_INITLEVEL    0x9

